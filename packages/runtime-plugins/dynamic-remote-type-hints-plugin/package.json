{
  "name": "@module-federation/dynamic-remote-type-hints-plugin",
  "version": "0.1.15",
  "license": "MIT",
  "description": "Support dynamic remote type hints in dev",
  "keywords": [
    "Module Federation",
    "runtime plugin",
    "dynamic remote type hints",
    "dev"
  ],
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "zhanghang <hanric.zhang@gmail.com>",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.cjs.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.cjs.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.cjs.d.ts"
      ]
    }
  },
  "dependencies": {
    "@module-federation/dts-plugin": "workspace:*"
  },
  "devDependencies": {
    "@module-federation/runtime": "workspace:*"
  }
}
